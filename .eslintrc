{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [ 2, { "ignore": [ "^[~]" ] }],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "new-cap": [2, { "capIsNewExceptions": ["List", "Map"] }]
  }
}
